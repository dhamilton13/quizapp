<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quizletclone"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="21" />

    <android:uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <android:uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <android:uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        android:debuggable="true"
        <activity
            android:name="com.example.quizletclone.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.quizletclone.ShortAnswer"
            android:label="@string/title_activity_create" >
        </activity>
        <activity
            android:name="com.example.quizletclone.ListActivity"
            android:label="@string/title_activity_list" >
        </activity>
        <activity
            android:name="com.example.quizletclone.FlashcardActivity"
            android:label="@string/title_activity_flashcard" >
        </activity>
        <activity
            android:name="com.example.quizletclone.TestListActivity"
            android:label="@string/title_activity_test_flashcard" >
        </activity>
        <activity android:name="com.example.quizletclone.MultipleChoice" >
        </activity>
        <activity android:name="com.example.quizletclone.TrueFalse" >
        </activity>
        <activity android:name="com.example.quizletclone.CheckAllThatApply" >
        </activity>
        <activity android:name="com.example.quizletclone.CreateTest" >
        </activity>
        <activity android:name="com.example.quizletclone.FlashcardListForTestsActivity" >
        </activity>
        <activity
            android:name="android.support.v7.widget.TestActivity"
            android:label="RecyclerViewTestActivity" />
    </application>

</manifest>